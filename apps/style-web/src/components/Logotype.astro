---
export interface Props {
  alt: string;
}

const { alt } = Astro.props;
---

<a href="/" class="kth-logotype">
  <figure>
    <img
      class="blue"
      alt={alt}
      width="64"
      height="64"
      src="/style/assets/logotype/logotype-blue.svg"
    />
    <img
      class="white"
      alt={alt}
      width="64"
      height="64"
      src="/style/assets/logotype/logotype-white.svg"
    />
  </figure>
</a>
