{
  "name": "root",
  "private": true,
  "workspaces": [
    "@kth/*",
    "apps/*"
  ],
  "scripts": {
    "generate-icons": "npx @svgr/cli --out-dir @kth/style/src/icons -- @kth/icons",
    "dev": "lerna run dev --stream --parallel --scope=@kth/style",
    "build": "lerna run build",
    "publish": "lerna publish --no-private"
  },
  "devDependencies": {
    "@svgr/cli": "^8.0.1",
    "@svgr/core": "^8.0.0",
    "lerna": "^6.4.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6"
  }
}
